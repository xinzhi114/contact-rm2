{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "linterOptions": {
    "exclude": ["src/common/**"]
  },
  "rules": {
    "prettier": true,
    "interface-name": false,
    "jsx-no-lambda": false,
    "no-empty": false
  }
}
